{
  "name": "myproject",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "pnpm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch:ts\" \"pnpm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "db:ingest": "tsx src/scripts/shell-ingest.ts",
    "db:destroy": "tsx src/scripts/shell-db-collection-destroy.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/sensible": "^6.0.0",
    "fastify-cli": "^7.4.0",
    "@chroma-core/default-embed": "^0.1.8",
    "@chroma-core/ollama": "^0.1.7",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/multipart": "^9.2.1",
    "@fastify/static": "^8.2.0",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.55",
    "@langchain/ollama": "^0.2.4",
    "@langchain/textsplitters": "^0.1.0",
    "chromadb": "^3.0.17",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "gray-matter": "^4.0.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "ts-node": "^10.4.0",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "typescript": "~5.8.2"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd"
}
