{
  "name": "myproject",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "fastify start -l info dist/app.js",
    "build": "tsc",
    "watch:ts": "tsc -w",
    "ts:dev": "tsx watch src/app.ts",
    "ts:build": "tsc",
    "dev": "pnpm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch:ts\" \"pnpm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "db:ingest": "tsx src/scripts/shell-ingest.ts",
    "db:destroy": "tsx src/scripts/shell-db-collection-destroy.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@chroma-core/ollama": "^0.1.7",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/multipart": "^9.2.1",
    "@fastify/sensible": "^6.0.0",
    "@fastify/static": "^8.2.0",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.55",
    "@langchain/ollama": "^0.2.4",
    "@langchain/textsplitters": "^0.1.0",
    "chromadb": "^3.0.17",
    "fastify": "^5.6.1",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.1.0",
    "gray-matter": "^4.0.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
